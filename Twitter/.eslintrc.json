{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true,
        "allowAfterSuper": true,
        "allow": ["_getPage", "_get", "_user", "_validateGeneral"]
      }
    ],
    "import/extensions": ["error", "never", { "js": "always" }],
    "no-param-reassign": ["error", { "props": false }],
    "operator-linebreak": [
      "error",
      "after",
      { "overrides": { "&&": "ignore" } }
    ]
  },
  "plugins": ["prettier"]
}
